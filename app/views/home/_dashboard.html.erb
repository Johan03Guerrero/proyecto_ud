    <li class="nav-item" role="presentation">
      <button class="nav-link <%= 'active' if params[:active] == 'proyeccion'%>" id="pills-proyeccion-tab" data-bs-toggle="pill" data-bs-target="#pills-proyeccion" type="button" role="tab" aria-controls="pills-personalizado" aria-selected="false">Proyecci√≥n</button>
    </li>
  </ul>
  <div class="tab-content" id="pills-tabContent">
    <div class="tab-pane fade <%= 'show active' if params[:active] == 'principal'%>" id="pills-principal" role="tabpanel" aria-labelledby="pills-principal-tab">
      <%= render partial: 'dashboard_principal' %>
    </div>
    <div class="tab-pane fade <%= 'show active' if params[:active] == 'personalizada'%> " id="pills-personalizado" role="tabpanel" aria-labelledby="pills-personalizado-tab">
      <%= render partial: 'dashboard_personalizado' %>
    </div>
   <div class="tab-pane fade <%= 'show active' if params[:active] == 'proyeccion'%> " id="pills-proyeccion" role="tabpanel" aria-labelledby="pills-personalizado-tab">
      <%= render partial: 'dashboard_proyeccion' %>
