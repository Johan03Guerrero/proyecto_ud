<% params[:active] ||= 'principal' %>
<div class="div-principal">
  <%= render "layouts/flash_messages" %>
  <ul class="nav nav-pills nav-fill" id="pills-tab" role="tablist">
    <li class="nav-item" role="presentation">
      <button class="nav-link <%= 'active' if params[:active] == 'principal'%>" id="pills-principal-tab" data-bs-toggle="pill" data-bs-target="#pills-principal" type="button" role="tab" aria-controls="pills-principal" aria-selected="true">Informes Principales</button>
    </li>
    <li class="nav-item" role="presentation">
      <button class="nav-link <%= 'active' if params[:active] == 'personalizada'%>" id="pills-personalizado-tab" data-bs-toggle="pill" data-bs-target="#pills-personalizado" type="button" role="tab" aria-controls="pills-personalizado" aria-selected="false">Generar Informe Personalizado</button>
